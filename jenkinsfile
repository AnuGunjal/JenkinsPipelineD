pipeline {
    agent any

    stages {
        stage('Build') {
            steps {
                echo 'Building'
            }
        }
        stage('Deploy') {
            steps {
                echo 'Deploying build'
            }
        }
        stage('Test') {
            steps {
                echo 'Testing build'
            }
        }
        stage('Release') {
            steps {
                echo 'Releasing build'
            }
        }
    }
}
